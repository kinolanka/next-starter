    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "4.14.195",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "5.14.6",
    "autoprefixer": "10.4.15",
    "clsx": "1.2.1",
    "eslint": "8.48.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.2",
