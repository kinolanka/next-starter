  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\" --max-warnings=0",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint-staged": "lint-staged",
    "setup": "chmod -R +x ./bin && ./bin/setup.sh"
  },
  "dependencies": {
    "@kinolanka/eslint-config": "0.0.1",
    "@kinolanka/prettier-config": "1.0.1",
    "@kinolanka/stylelint-config": "0.0.1",
