{
  "name": "next-starter",
  "version": "0.0.0",
  "private": true,
  "author": "Volodymyr Mashkin",
  "description": "This is a Next.js starter project bootstrapped with create-next-app.",
  "repository": "https://github.com/kinolanka/next-starter",
  "license": "ISC",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\" --max-warnings=0",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint-staged": "lint-staged",
    "setup": "chmod -R +x ./bin && ./bin/setup.sh",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "clsx": "1.2.1",
    "cookies-next": "2.1.1",
    "lodash": "4.17.21",
    "next": "13.1.6",
    "next-seo": "5.15.0",
    "next-sitemap": "3.1.52",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/lodash": "4.14.191",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-testing-library": "5.10.2",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "13.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "stylelint": "15.1.0",
    "stylelint-config-standard": "30.0.1",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14.20 <15",
    "npm": ">=6.14 <7"
  }
